<table>
  <thead>
    <tr>
      <th>注文日</th>
      <th>配送先</th>
      <th>注文商品</th>
      <th>支払金額</th>
      <th>ステータス</th>
      <th>注文詳細</th>
    </tr>
  </thead>
  <tbody>
    <tbody>
      <% @orders.each do |order| %>
        <tr>
          <td><%= order.created_at %></td>
          <td><%= order.address %></td>
          <td><%#= order %></td>
          <td><%= order.total_payment %>円</td>
          <td><%= order.status_i18n %></td>
          <td><%= link_to "表示する" %></td>
        </tr>
      <% end %>
    </tbody>
  </tbody>
</table>